{
    "window.title": "${activeEditorLong}${separator}${rootName}",
    "window.zoomLevel": 0,
    "editor.tabSize": 2,
    "git.autofetch": true,
    "editor.fontFamily": "'Fira Code'",
    "editor.fontLigatures": true,
    "editor.fontSize": 15,
    "editor.wordWrap": "on",
    "files.associations": {
        "Jenkinsfile": "groovy",
        "Fastfile": "ruby",
        "Podfile": "ruby",
        "Matchfile": "ruby",
        "Deliverfile": "ruby",
        "Gymfile": "ruby",
        "Pluginfile": "ruby",
        "Snapfile": "ruby",
        "commands_*.txt": "ruby",
        "commands_shell.txt": "shellscript",
        "commands_docker.txt": "shellscript",
        "commands_jenkins.txt": "groovy",
        "*.command": "shellscript",
        ".bash_profile": "shellscript",
        ".bash_history": "shellscript",
    },
    "workbench.iconTheme": "material-icon-theme",
    "csharp.suppressDotnetInstallWarning": true,
    "dart.previewFlutterUiGuides": true,
}
